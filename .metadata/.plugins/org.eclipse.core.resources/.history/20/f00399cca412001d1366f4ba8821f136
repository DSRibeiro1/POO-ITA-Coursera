import java.util.Scanner;
public class Teste { //folha2. MediaFaltas

	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		
		System.out.println("Digite nota 1: ");
		double n1 = ler.nextDouble();
		System.out.println("Digite nota 2:");
		double n2 = ler.nextDouble();
		System.out.println("Digite nota 3:");
		double n3 = ler.nextDouble();
		
		
		
		mostrarMedia(n1,n2,n3);

		mostrarResultado(n1,n2,n3);

	}	

	
	public static void mostrarMedia(double n1, double n2, double n3) {
		double media = (n1+n2+n3)/3;
		System.out.println("Média: "+media);
	}
	
	
	public static void mostrarResultado(double n1, double n2, double n3) {
		Scanner ler = new Scanner(System.in);
		
		double media = (n1+n2+n3)/3;
		System.out.print("Digite faltas: ");
		int faltas = Integer.parseInt(ler.nextLine());
		
		if (media >=7 && faltas < 6) {
			System.out.println("Aprovado");
		
		}else if (media > 4 && media < 7 && faltas < 6) {
			System.out.println("Recuperação");
		
		}else {
			System.out.println("Reprovado");
		}
	}
}
	
	
		

	
	
	
		
		
		
		
		
		

		
		
	
		
		
		
	
	
	
		
		
	
	
			
		
		
		
		
				
				
		
	

			
		

		
		
		
		
		
	
		
		
		
	

		
	




		
	

		
	
		
		

		
			
		
		
		
		
	
	
		
		
	

	



	
	

		
	
		
		
		
		
		
			
		
	







	

			
				
			
			
					
		
		
		

	

		
			
		
	

				
			
		
	
			
		
		
		
		
		
		
		


	
		
		
		




		
		
	
		
		
		
		
		
		
		
		
		
	
		
			      
				





	



	

	

			
		




	
		

		
		
		
		
		
		
		
				
		
			
		
		
		
		
		
		

	
		
	

		


		
		
		
			
		
		
		
	

		


		
		
		
				
		




		
		
		
		
		
		
		
		
			
		
	





		
		
		
		
				
		
		
		
		
	
	




	
	
	
		
		
		
		



	
			
		
		
		
		

	


	
	

				
		
	

		

		
	
				
		
		
		

			
				
				
		
		
				
			
		

	
		
		

	















	



	





	
		
			
				
			
			


			


		

	


