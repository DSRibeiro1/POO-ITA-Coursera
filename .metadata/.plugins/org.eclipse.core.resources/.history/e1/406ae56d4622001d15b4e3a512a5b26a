package Teste;
import java.util.Scanner;

public class Teste {
	
	final static int N = 5;

	public static void main(String[] args) {
		
		Teste test = new Teste();
		
		lerDados(test);
		double somaPosit = mostrarPositivo(test);
		quantNegativo();
	}
	
	
	public static void lerDados(Teste test) {
		
		Scanner console = new Scanner(System.in);
		for(test.i=0;test.i<N;test.i++) { 
			System.out.println("Digite número: ");
			test.n[test.i] = console.nextDouble();
			
		}
	}
	public static double mostrarPositivo(TesteOO test) {
		
		for(test.i=0;test.i<N;test.i++) {
			double soma = 0;
			if(test.n[test.i) > 0 {
				somaPosit = soma+test.n[test.i];
			}
			return somaPosit;
		}
	}
		
		
		
		
		
		
		
		

	}

}
